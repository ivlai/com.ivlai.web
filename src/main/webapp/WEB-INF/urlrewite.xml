<?xml version="1.0" encoding="UTF-8"?>
<urlrewrite>
    <rule>
        <name>seo redirect</name>
        <condition name="host" operator="notequal">^ivlai.com</condition>
        <!--<condition name="host" operator="notequal">^localhost</condition>-->
        <from>^/(.*)</from>
        <to type="permanent-redirect" last="true">http://www.ivlai.com/$1</to>
    </rule>
</urlrewrite>